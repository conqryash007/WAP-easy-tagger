{
  "manifest_version": 2,
  "name": "tagWhatsapp",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "WhatsApp Tagger"
  }
}
